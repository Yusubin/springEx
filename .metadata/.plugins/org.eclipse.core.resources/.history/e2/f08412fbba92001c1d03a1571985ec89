<mapper namespace="bbs">
	<insert id="create" parameterType="memberVO">
		insert into MEMBER values (#{id},#{writer},#{title},#{tel})
	</insert>
	
	
	<select id="check2" parameterType="memberVO" resultType="memberVO">
		select id from MEMBER where id = #{id}
	</select>
	
