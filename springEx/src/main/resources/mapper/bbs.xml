<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="bbs">
	<select id="one" parameterType="bbsVO" 
	resultType="bbsVO">
		select * from bbs where id = #{id}
	</select>

	<select id="list" resultType="bbsVO">
		select * from bbs
	</select>
	<insert id="create" parameterType="bbsVO">
		insert into bbs values (#{id},#{writer},#{title},#{contents})	
		<!-- read 빼고 //return은 무조건 int -->
	</insert>
	
	
	
	<select id="check2" parameterType="bbsVO" resultType="bbsVO">
		select id from bbs where id = #{id}
	</select>
	
</mapper>
